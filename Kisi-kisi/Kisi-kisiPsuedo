Deklarasi:
a, b, c;

Deskripsi:
if (a>10), statement
	if (b<15),print statement c = a + b; a = b; b = c; total = c + a + b
	else, print statement c = a * b; a = c;b = c; total = a + b + c 
else, print statement c = a-b; b = c; a = b; total = b + c + a
write a, b, c, total